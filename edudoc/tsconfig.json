{
    "angularCompilerOptions": {
        "preserveWhitespaces": true
    },
    "compileOnSave": false,
    "compilerOptions": {
      "importHelpers": true,
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "paths": {
        "@model/*": [
          "src/app/model/*"
        ],
        "@common/*": [
          "src/app/common/*"
        ],
        "@admin/*": [
          "src/app/admin-portal/*"
        ],
        "@school-district-admin/*": [
          "src/app/school-district-admin-portal/*"
        ],
        "@provider/*": [
          "src/app/provider-portal/*"
        ]
      },
      "experimentalDecorators": true,
      "noImplicitReturns": true,
      "target": "ES2022",
      "typeRoots": [
        "node_modules/@types"
      ],
      "lib": [
        "ES2019",
        "es2018",
        "es2017",
        "dom"
      ],
      "module": "esnext",
      "baseUrl": "./",
      "esModuleInterop": true,
      "useDefineForClassFields": false
    }
}
